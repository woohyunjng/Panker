model User {
    id        Int      @id @default(autoincrement())
    email     String   @unique
    name      String
    status    String?
    school    School  @relation(fields: [schoolId], references: [id])
    schoolId  Int
}

model School {
    id        Int      @id @default(autoincrement())
    name      String
    manager   User     @relation(fields: [managerId], references: [id])
    managerId Int
    students  User[]
}